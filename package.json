{
  "name": "whoisens-lib",
  "version": "1.0.0-beta.7",
  "license": "MIT",
  "author": "Alex Ivasyuv",
  "homepage": "https://github.com/whoisens/whoisens-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/whoisens/whoisens-lib.git"
  },
  "bugs": "https://github.com/whoisens/whoisens-lib/issues",
  "description": "Whois for ENS (Ethereum Name Service)",
  "keywords": [
    "ENS",
    "ethereum",
    "Ethereum Name Service",
    "whois",
    "resolve",
    "reverse resolve",
    "contenthash",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build:esm": "npx tsc",
    "build:cjs": "npx tsc --target es5 --module commonjs --outDir dist/cjs",
    "build:watch": "rm -rf ./dist && npx tsc -w",
    "test": "rm -rf ./dist && npm run build:esm && npm run test:only",
    "test:only": "node --experimental-modules --experimental-json-modules tests/index.js",
    "docs": "rm -rf ./api-docs && npx typedoc --out api-docs",
    "prepublishOnly": "rm -rf ./dist && npm run build:esm && npm run build:cjs && npm run test:only"
  },
  "dependencies": {
    "content-hash": "^2.3.2",
    "eth-ens-namehash": "^2.0.8",
    "js-sha3": "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "node-fetch": "^2.6.0",
    "node-test": "^1.2.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "whoisens-test-dataset": "^0.1.0"
  }
}
